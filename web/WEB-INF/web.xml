<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">



    <servlet>
        <servlet-name>Authentification</servlet-name>
        <servlet-class>User.Serve_Authentication</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Authentification</servlet-name>
        <url-pattern>/Serve_Authentication</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>User.Serve_Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Serve_Registration</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Serve_Logout</servlet-name>
        <servlet-class>User.Serve_Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Serve_Logout</servlet-name>
        <url-pattern>/Serve_Logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Serve_Changepassword</servlet-name>
        <servlet-class>User.Serve_Changepassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Serve_Changepassword</servlet-name>
        <url-pattern>/Serve_Changepassword</url-pattern>
    </servlet-mapping>



</web-app>